# Pin assignments for 10 LEDs on the DE1-SoC board
set_location_assignment PIN_V16 -to leds[0]  ;# LEDR0
set_location_assignment PIN_W16 -to leds[1]  ;# LEDR1
set_location_assignment PIN_V17 -to leds[2]  ;# LEDR2
set_location_assignment PIN_V18 -to leds[3]  ;# LEDR3
set_location_assignment PIN_W17 -to leds[4]  ;# LEDR4
set_location_assignment PIN_W19 -to leds[5]  ;# LEDR5
set_location_assignment PIN_Y19 -to leds[6]  ;# LEDR6
set_location_assignment PIN_W20 -to leds[7]  ;# LEDR7
set_location_assignment PIN_W21 -to leds[8]  ;# LEDR8
set_location_assignment PIN_Y21 -to leds[9]  ;# LEDR9
